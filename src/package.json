{
  "name": "typescript-react-skeleton",
  "version": "0.1.0",
  "description": "Typescript and React project skeleton",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf ../build && mkdir ../build",
    "build:webpack": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "webpack --config config/webpack.dev.js && node dev_server.js",
    "lint": "npm run lint-only && ./tools/check-todos.sh",
    "lint-only": "./tools/tslint.sh && ./tools/check-semicolons.sh && ./tools/check-pure-render.sh && ./tools/scsslint.sh && ./tools/check-white-space.sh",
    "tsconfig": "node ./tools/generate-tsconfig.js tsconfig.json",
    "test": "rimraf ts-build && tsc && NODE_ENV=test mocha --compilers js:babel-core/register test/mocha/mocha-noop-compiler.js test/mocha/mocha-css-module-compiler.js --require ./test/testHelper.js ts-build/**/*.spec.js",
    "pull-request": "npm test && npm run lint"
  },
  "author": "Tom Duncalf",
  "license": "ISC",
  "dependencies": {
    "history": "2.0.0-rc2",
    "lodash": "4.1.0",
    "react": "0.14.7",
    "react-addons-perf": "0.14.7",
    "react-addons-test-utils": "0.14.7",
    "react-css-modules": "3.6.8",
    "react-dom": "0.14.7",
    "react-redux": "4.1.2",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "2.1.0",
    "redux": "3.1.7",
    "redux-logger": "2.4.0",
    "redux-router": "1.0.0-beta7",
    "redux-thunk": "1.0.3",
    "shallowequal": "0.2.2"
  },
  "devDependencies": {
    "autoprefixer": "6.3.1",
    "awesome-typescript-loader": "0.15.10",
    "babel": "6.3.26",
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-runtime": "6.3.19",
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "chai-jsx": "1.0.1",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "2.1.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "glob": "6.0.4",
    "html-webpack-plugin": "2.7.2",
    "jsdom": "8.0.2",
    "mocha": "2.4.5",
    "postcss": "5.0.14",
    "postcss-loader": "0.8.0",
    "postcss-modules-extract-imports": "1.0.0",
    "postcss-modules-local-by-default": "1.0.1",
    "postcss-modules-scope": "1.0.0",
    "postcss-modules-values": "1.1.1",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "redux-devtools": "3.0.2",
    "redux-devtools-dock-monitor": "1.0.1",
    "redux-devtools-log-monitor": "1.0.2",
    "redux-slider-monitor": "1.0.1",
    "rimraf": "2.5.1",
    "sinon": "1.17.3",
    "sinon-as-promised": "4.0.0",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.0",
    "tslint": "3.3.0",
    "typescript": "1.8.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.12",
